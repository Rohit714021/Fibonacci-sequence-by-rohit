#Fibonacci sequence 
nterms = int(input("How many terms? "))

a1, a2 = 0, 1
count = 0

if nterms == 1:
   print("Fibonacci sequence upto",nterms,":")
   print(a1)
else:
   print("Fibonacci sequence:")
   while count < nterms:
       print(a1)
       nth = a1 + a2
       a1 = a2
       a2 = nth
       count += 1
